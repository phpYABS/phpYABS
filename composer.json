{
    "name": "phpyabs/phpyabs",
    "description": "Open source used book managment software. Ugly code, it was my very first project in 2003",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Davide Bellettini",
            "email": "davide.bellettini@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-bcmath": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "^3.3",
        "doctrine/doctrine-bundle": "^2.14",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.3",
        "moneyphp/money": "^4.6",
        "pentatrion/vite-bundle": "^8.0",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.1",
        "ronanguilloux/isocodes": "^2.3",
        "siburuxue/doctrine-helper": "^1.0",
        "symfony/flex": "^2.5",
        "symfony/form": "^7.2",
        "symfony/framework-bundle": "^7.2",
        "symfony/property-access": "^7.2",
        "symfony/property-info": "^7.2",
        "symfony/psr-http-message-bridge": "^7.2",
        "symfony/runtime": "^7.2",
        "symfony/security-csrf": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/stimulus-bundle": "^2.23",
        "symfony/translation": "^7.2",
        "symfony/twig-bundle": "^7.2",
        "symfony/uid": "^7.2",
        "symfony/ux-translator": "^2.23",
        "symfony/ux-vue": "^2.23",
        "symfony/validator": "^7.2",
        "symfony/var-dumper": "^7.2",
        "symfony/yaml": "^7.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.20",
        "twig/twig": "^2.12|^3.0",
        "yceruto/money-bundle": "^1.3"
    },
    "require-dev": {
        "behat/behat": "^3.19",
        "behat/mink": "^1.12",
        "behat/mink-browserkit-driver": "^2.2",
        "ergebnis/composer-normalize": "^2.42",
        "friends-of-behat/mink-extension": "^2.7",
        "friends-of-behat/symfony-extension": "^2.6",
        "friendsofphp/php-cs-fixer": "^3.73",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.0",
        "rector/rector": "^2.0",
        "symfony/maker-bundle": "^1.62"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "PhpYabs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpYabs\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
