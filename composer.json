{
    "name": "phpyabs/phpyabs",
    "description": "Open source used book managment software. Ugly code, it was my very first project in 2003",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Davide Bellettini",
            "email": "davide.bellettini@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-pdo": "*",
        "doctrine/dbal": "^3.3",
        "doctrine/doctrine-bundle": "^2.14",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.3",
        "monolog/monolog": "^3.8",
        "php-di/php-di": "^7.0",
        "ronanguilloux/isocodes": "^2.3",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.9",
        "slim/twig-view": "^3.3",
        "symfony/flex": "^2.5",
        "symfony/framework-bundle": "^7.2",
        "symfony/psr-http-message-bridge": "^7.2",
        "symfony/runtime": "^7.2",
        "symfony/translation": "^7.2",
        "symfony/twig-bundle": "^7.2",
        "symfony/var-dumper": "^7.2",
        "symfony/yaml": "^7.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.42",
        "friendsofphp/php-cs-fixer": "^3.73",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.0",
        "rector/rector": "^2.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "PhpYabs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpYabs\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
