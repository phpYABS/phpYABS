<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}phpYABS - menu{% endblock title %}</title>
    {% block stylesheets %}
        {{ vite_entry_link_tags('app') }}
    {% endblock %}
    {% block javascripts %}
        {{ vite_entry_script_tags('app') }}
    {% endblock %}
</head>
<body {% block body_attr %}{% endblock %}>
{% block body %}
    <header>
        <nav id="menu">
            {% include 'menu.html.twig' %}
        </nav>
    </header>
    <main id="content">
        {% block content %}
        {% endblock content %}
    </main>
{% endblock body %}
</body>
</html>
